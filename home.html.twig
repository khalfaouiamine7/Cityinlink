<style>
    /* Définir les couleurs par défaut (mode clair) */
    :root {
        --primary-color: #731010; /* Rouge personnalisé */
        --background-color: #F0FBFC; /* Fond clair */
        --text-color: #000; /* Texte noir */
        --navbar-background-color: #fff; /* Fond de la barre de navigation */
    }

    /* Styles pour le mode sombre */
    @media (prefers-color-scheme: dark), (body.dark-mode) {
        :root {
            --primary-color: #FF6347; /* Tomate */
            --background-color: #333; /* Fond sombre */
            --text-color: #fff; /* Texte blanc */
            --navbar-background-color: #222; /* Fond de la barre de navigation sombre */
        }
    }

    body {
        background-color: var(--background-color);
        color: var(--text-color);
    }

    .navbar {
        background-color: var(--navbar-background-color);
    }

    .text-primary {
        color: var(--primary-color) !important;
    }

    .btn-primary {
        background-color: var(--primary-color);
        border-color: var(--primary-color);
    }
</style>

