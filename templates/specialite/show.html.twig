{% extends 'back.html.twig' %}

{% block title %}Specialite{% endblock %}

{% block body %}
<div class="container">
    <h1 class="mt-4 mb-3">Specialite</h1>
    <div class="row">
        <div class="col-lg-12">
            <table class="table table-bordered">
                <tbody>
                    <tr>
                        <th scope="row">Id</th>
                        <td>{{ specialite.id }}</td>
                    </tr>
                    <tr>
                        <th scope="row">Nom</th>
                        <td>{{ specialite.nom }}</td>
                    </tr>
                    <tr>
                        <th scope="row">Description</th>
                        <td>{{ specialite.description }}</td>
                    </tr>
                    <tr>
                        <th scope="row">Type</th>
                        <td>{{ specialite.type }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <a href="{{ path('app_specialite_index') }}" class="btn btn-primary">Back to List</a>
            <a href="{{ path('app_specialite_edit', {'id': specialite.id}) }}" class="btn btn-info">Edit</a>
            {{ include('specialite/_delete_form.html.twig') }}
        </div>
    </div>
</div>
{% endblock %}
