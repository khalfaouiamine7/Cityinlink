{% extends 'basefront.html.twig' %}

{% block title %}Medicament{% endblock %}

{% block body %}
    <h1>Medicament</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdMedicament</th>
                <td>{{ medicament.idMedicament }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ medicament.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ medicament.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ medicament.prix }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ medicament.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_medicament_index') }}">back to list</a>

    <a href="{{ path('app_medicament_edit', {'idMedicament': medicament.idMedicament}) }}">edit</a>

    {{ include('medicament/_delete_form.html.twig') }}
{% endblock %}
